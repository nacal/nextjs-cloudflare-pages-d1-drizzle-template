{
  "name": "nextjs-cloudflare-pages-d1-drizzle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:seed:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) tsx ./db/seed.ts",
    "db:migrate:local": "wrangler d1 migrations apply my-database-name --local",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview my-database-name --remote",
    "db:migrate:prod": "wrangler d1 migrations apply my-database-name --remote",
    "db:studio:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env NODE_ENV='preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "pages:build": "pnpm next-on-pages",
    "dev": "next dev -p 3333",
    "build": "next build",
    "pages:dev": "pnpm pages:build && pnpm wrangler pages dev",
    "pages:deploy:preview": "pnpm pages:build && pnpm wrangler pages deploy --branch preview",
    "pages:deploy": "pnpm pages:build && pnpm wrangler pages deploy"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@radix-ui/themes": "^3.2.1",
    "@react-hookz/web": "^25.1.1",
    "@tanstack/react-query": "^5.74.9",
    "@tanstack/react-query-next-experimental": "^5.74.9",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "next": "15.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/next-on-pages": "^1.13.12",
    "@cloudflare/workers-types": "^4.20250429.0",
    "@next/env": "^15.3.1",
    "@t3-oss/env-nextjs": "^0.13.0",
    "@tanstack/react-query-devtools": "^5.74.9",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "better-sqlite3": "^11.9.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unplugin-turbo-console": "^2.1.3",
    "wrangler": "^4.13.2"
  }
}
